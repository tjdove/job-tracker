{
  "name": "job-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "tsx watch -r dotenv/config index.ts",
    "introspect": "drizzle-kit introspect:mysql --config=drizzle.config.ts",
    "generate": "drizzle-kit generate:mysql --config=drizzle.config.ts",
    "startServer": "npx nodemon ./server/app.js"
  },
  "dependencies": {
    "@planetscale/database": "^1.11.0",
    "axios": "^1.5.1",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "express": "^4.18.2",
    "mysql2": "^3.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^3.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.16",
    "daisyui": "^3.9.2",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
